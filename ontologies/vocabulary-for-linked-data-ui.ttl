<http://data.sparna.fr/ontologies/volui> rdf:type owl:Ontology ;
        dct:created "2023-04-22"^^xsd:date ;
        dct:creator <mailto:thomas.francart@sparna.fr> ;
        dct:license <https://creativecommons.org/publicdomain/zero/1.0/>;
      vann:preferredNamespacePrefix "volui" ;
      rdfs:comment "A lightweight vocabulary to annotate linked data resources with user interface related information. Properties in this vocabulary can be used on individuals, classes or properties. It is up to a user interface which wants to draw on them to pick how it uses this information. For example, the style attribute of a class may be picked to distinguish information about instances of that class, or to display the class itself, or to display properties having this class as their domain."@en ;
      rdfs:label "VOcabulary for Linked data User Interfaces"@en ;
      skos:altLabel "volui"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://data.sparna.fr/ontologies/volui#icon
volui:icon rdf:type owl:ObjectProperty ;
           rdfs:comment "An icon resource associated with the resource."@en ;
           rdfs:label "icon"@en ;
           rdfs:seeAlso volui:iconCode .

###  http://data.sparna.fr/ontologies/volui#icon
volui:iconCode rdf:type owl:DatatypeProperty ;
           rdfs:comment "The code of an icon in an icon library associated with the resource."@en ;
           rdfs:seeAlso volui:icon ;
           rdfs:label "icon code"@en .




###  http://purl.org/vocab/vann/preferredNamespacePrefixpreferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#closeMatch
skos:closeMatch rdf:type owl:AnnotationProperty .


#################################################################
#    Data properties
#################################################################

###  http://data.sparna.fr/ontologies/volui#color
volui:color rdf:type owl:DatatypeProperty ;
            rdfs:comment "A color associated with the ressource, typically to indicate the background or text color in which the resource should be displayed. Must be a valid CSS color string such as one could put in a CSS 'color' attribute."@en ;
            rdfs:label "color"@en ;
            owl:equivalentProperty <http://www.w3.org/ns/ui#color> ;
            rdfs:datatype xsd:string .


###  http://data.sparna.fr/ontologies/volui#sequence
volui:sequence rdf:type owl:DatatypeProperty ;
               rdfs:comment "The sequence in which this resource is arranged with respect to others."@en ;
               rdfs:label "sequence number"@en ;
               skos:closeMatch <http://open.vocab.org/terms/rank> ,
                               <http://www.w3.org/ns/ui#seqeunce> .


###  http://data.sparna.fr/ontologies/volui#tooltip
volui:tooltip rdf:type owl:DatatypeProperty ;
              rdfs:comment "A tooltip associated with the ressource."@en ;
              rdfs:label "tooltip"@en ;
              skos:closeMatch <http://www.w3.org/ns/ui#prompt> .

volui:cssClass rdf:type owl:DatatypeProperty ;
            rdfs:comment "A CSS class associated with the resource, typically to indicate the CSS class to use when displaying the resource. Must be a valid CSS class name string such as one would put in an HTML 'class' attribute"@en ;
            rdfs:label "CSS class"@en ;
            rdfs:datatype xsd:string .

volui:style rdf:type owl:DatatypeProperty ;
            rdfs:comment "A valid CSS style string such as one could put in an HTML 'style' attribute, e.g. 'color: red;'."@en ;
            owl:equivalentProperty <http://www.w3.org/ns/ui#style> ;
            rdfs:label "CSS style"@en ;
            rdfs:datatype xsd:string .


